{{#each listenlist}}
<html lang="en">
	<head>
		<meta charset="UTF-8">
		<title>Songs Listing</title>
	</head>
	<body>

		<h1>Songs Listing</h1>
			<table border="1px">
                <tr>
                    <th>Flag</th>
                    <th>Title</th>
                    <th>Country</th>
                    <th></th>
                </tr>
				<tr>
					<td><img src ="images/{{ country }}.png" style="height: 20px; width: auto;"></src></td>
					<td>{{ title }}</td>
                    <td>{{ country }}</td>
					 {{@key}}: {{check_out}} ==0
                    	<td>Not available</td>
					 {{@key}}: {{check_out}} >0
					 	<td><button type ="submit">Listen</button></td>					
                </tr>
            </table>

		
	</body>
</html>
{{/each}}